@model IEnumerable<DebReg.Models.OrganizationUser>
@if (Model.Count() > 0) {
    foreach (var organizationUser in Model) {
        <div class="row">
            <div class="col-sm-12">
                <strong>
                    @Html.DisplayFor(model => organizationUser.User.FirstName)
                    @Html.DisplayFor(model => organizationUser.User.LastName)
                </strong>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Html.DisplayFor(model => organizationUser.Role)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Html.DisplayFor(model => organizationUser.User.Email)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Html.DisplayFor(model => organizationUser.User.PhoneNumber)
            </div>
        </div>
    }
}
